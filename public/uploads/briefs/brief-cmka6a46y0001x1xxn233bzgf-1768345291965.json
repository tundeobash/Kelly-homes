    "accent": [
      "soft_blue",
      "sage_green",
      "gold",
      "silver"
    ]
  },
  "furniture": [
    {
      "type": "sofa",
      "styleNotes": "Upholstered with clean lines and comfortable cushions, track or subtly rolled arms.",
